{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "name": "twatair",
    "buildCommand": null,
    "outputDirectory": ".",
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, OPTIONS"
                },
                {
                    "key": "Cache-Control",
                    "value": "s-maxage=60, stale-while-revalidate"
                }
            ]
        }
    ]
}
